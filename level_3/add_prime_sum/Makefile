SRC = my_add_prime_sum.c

all:
	@cc -g -Wall -Wextra -Werror $(SRC) -o my_add_prime_sum
	@./my_add_prime_sum 0
	#@echo 0
	@./my_add_prime_sum 5
	#@echo 10
	@./my_add_prime_sum 7 | cat -e
	#@echo 17$
	@./my_add_prime_sum 100 | cat -e
	#@echo 1060$
	@./my_add_prime_sum 1000 | cat -e
	#@echo 76127$
	@./my_add_prime_sum 10000 | cat -e
	#@echo 5736396$
	@./my_add_prime_sum 100000 | cat -e
	#@echo 454396537$
	@./my_add_prime_sum 1000000 | cat -e
	#@echo 37550402023$
	@./my_add_prime_sum 10000000 | cat -e
	#@echo 3203324994356$
	@./my_add_prime_sum 100000000 | cat -e
	#@echo 282332397870031$
	@./my_add_prime_sum 1000000000 | cat -e
	#@echo 25401694053278021$
	@./my_add_prime_sum 10000000000 | cat -e
	#@echo 232792560$
	@./my_add_prime_sum 100000000000 | cat -e
	#@echo 216160011410907677$
	@./my_add_prime_sum 1000000000000 | cat -e
	#@echo 20365011074$
	@./my_add_prime_sum 10000000000000 | cat -e
	#@echo 1932053504$
	@./my_add_prime_sum 100000000000000 | cat -e
	#@echo 18446744073709551615$
	@./my_add_prime_sum 1000000000000000 | cat -e
	#@echo 1779979416004714189$
	@./my_add_prime_sum 10000000000000000 | cat -e
	#@echo 173402521172797813$
	@./my_add_prime_sum 100000000000000000 | cat -e
